{
  "name": "gossembrot-db",
  "version": "1.21.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:test": "ng build --configuration=test-server && ./copy-htaccess.sh test-server",
    "build:prod": "ng build --configuration=production && ./copy-htaccess.sh production",
    "build:static": "ng build --configuration static",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "api": "node api/local-server.js",
    "registerPassword": "node registerPassword.js",
    "compodoc": "npx compodoc -p tsconfig.doc.json",
    "export-structure-png": "npx dependency-cruiser --exclude \"node_modules\" --include-only \"\\.ts$\" --output-type dot src/ > dependency-graph.dot && dot -Tpng -o dependency-graph.png dependency-graph.dot",
    "generate-docs": "npm run compodoc && npm run export-structure-png",
    "lint": "ng lint",
    "format": "prettier --write 'src/**/*.ts'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@angular/service-worker": "^17.3.12",
    "@auth0/angular-jwt": "^5.2.0",
    "@ng-matero/extensions": "^17.3.7",
    "@ngxs/store": "^3.8.2",
    "@types/openseadragon": "^3.0.10",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.2",
    "material-design-icons": "^3.0.1",
    "openseadragon": "^4.1.1",
    "rxjs": "^7.8.1",
    "sass": "^1.80.3",
    "sql.js": "^1.13.0",
    "tslib": "^2.8.0",
    "zone.js": "^0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.11",
    "@angular-eslint/builder": "^17.5.3",
    "@angular-eslint/eslint-plugin": "^17.5.3",
    "@angular-eslint/eslint-plugin-template": "^17.5.3",
    "@angular-eslint/schematics": "^17.5.3",
    "@angular-eslint/template-parser": "17.5.3",
    "@angular/cli": "^17.3.10",
    "@angular/compiler-cli": "^17.3.12",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "~4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.62.0",
    "dependency-cruiser": "^13.0.5",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "mysql2": "^3.5.0",
    "prettier": "^3.0.0",
    "typescript": "~5.4.5",
    "webpack": "^5.92.1"
  }
}
