{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "gossembrot-portal": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/gossembrot-portal",
            "baseHref": "/gossembrot-portal/",
            "index": "src/index.html",
            "main": "src/main.ts",
            "optimization": true,
            "buildOptimizer": true,
            "aot": true,
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/images",
              "src/Lagensymbole",
              {
                "glob": "**/*",
                "input": "src/images",
                "output": "/images/"
              },
              "src/manifest.webmanifest",
            ],
            "styles": [
              "src/styles.scss",
              "node_modules/material-design-icons/iconfont/material-icons.css"
            ],
            "scripts": [],
            "serviceWorker": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "100kb",
                  "maximumError": "200kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "test-server": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "6mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "100kb",
                  "maximumError": "200kb"
                }
              ],
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                },
                "fonts": true
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.test-server.ts"
                }
              ],
              "baseHref": "/gossembrot-portal_test/",
              "outputPath": "dist/gossembrot-portal_test",
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            },
            "static-apache": {
              "budgets": [
                { "type": "initial", "maximumWarning": "4mb", "maximumError": "6mb" },
                { "type": "anyComponentStyle", "maximumWarning": "100kb", "maximumError": "200kb" }
              ],
              "optimization": {
                "scripts": true,
                "styles": { "minify": true, "inlineCritical": false },
                "fonts": true
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.static-apache.ts"
                }
              ],
              "baseHref": "/gossembrot-portal_static/",
              "deployUrl": "/gossembrot-portal_static/",
              "outputPath": "dist/gossembrot-portal_static",
              "outputHashing": "all",

              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/images",
                "src/Lagensymbole",
                {
                  "glob": "**/*",
                  "input": "src/images",
                  "output": "images/"
                },
                "src/manifest.webmanifest",
                { "glob": "**/*", "input": "node_modules/sql.js/dist", "output": "assets/sqljs" },
                { "glob": "app.sqlite", "input": "src/assets/db", "output": "assets/db" },
                { "glob": "sw.js", "input": "src/static", "output": "" }
              ]
            },
            "static-local": {
              "budgets": [
                { "type": "initial", "maximumWarning": "4mb", "maximumError": "6mb" },
                { "type": "anyComponentStyle", "maximumWarning": "100kb", "maximumError": "200kb" }
              ],
              "optimization": {
                "scripts": true,
                "styles": { "minify": true, "inlineCritical": false },
                "fonts": true
              },
              "fileReplacements": [
                { "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.static-local.ts" }
              ],
              "baseHref": "/",
              "deployUrl": "/",
              "outputPath": "dist/gossembrot-portal_static",
              "outputHashing": "all",
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/Lagensymbole",
                { "glob": "sw.js",              "input": "src/static",         "output": "" },
                { "glob": "**/*",               "input": "node_modules/sql.js/dist", "output": "assets/sqljs" },
                { "glob": "app.sqlite",         "input": "src/assets/db",      "output": "assets/db" }
              ]
            },
            "ghpages": {
              "budgets": [
                { "type": "initial", "maximumWarning": "4mb", "maximumError": "6mb" },
                { "type": "anyComponentStyle", "maximumWarning": "100kb", "maximumError": "200kb" }
              ],
              "optimization": {
                "scripts": true,
                "styles": { "minify": true, "inlineCritical": false },
                "fonts": true
              },
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.ghpages.ts"
                }
              ],
              "baseHref": "./",
              "deployUrl": "./",
              "outputPath": "dist/gossembrot-portal_static",
              "outputHashing": "all",
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/images",
                "src/Lagensymbole",
                { "glob": "**/*", "input": "src/images", "output": "images/" },
                { "glob": "**/*", "input": "node_modules/sql.js/dist", "output": "assets/sqljs" },
                { "glob": "app.sqlite", "input": "src/assets/db", "output": "assets/db" },
                { "glob": "sw.js", "input": "src/static", "output": "/" }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "gossembrot-portal:build:production"
            },
            "development": {
              "buildTarget": "gossembrot-portal:build:development"
            },
            "static": {
              "buildTarget": "gossembrot-portal:build:static"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "proxyConfig": "proxy.conf.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "gossembrot-portal:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/images",
              "src/Lagensymbole",
              {
                "glob": "**/*",
                "input": "src/images",
                "output": "/images/"
              },
              "src/manifest.webmanifest",
              "src/manifest.webmanifest"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ],
    "analytics": false
  }
}
